<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:state_pressed="false">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/edit_location_suggestion_button_corner_radius"/>
            <solid android:color="?attr/edit_location_suggestion_background_color"/>
            <stroke android:width="?attr/edit_location_suggestion_stroke_width" android:color="?attr/edit_location_suggestion_stroke_color"/>
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/edit_location_suggestion_button_corner_radius"/>
            <solid android:color="?attr/edit_location_suggestion_stroke_color"/>
            <stroke android:width="?attr/edit_location_suggestion_stroke_width" android:color="?attr/edit_location_suggestion_stroke_color"/>
        </shape>
    </item>
</selector>
