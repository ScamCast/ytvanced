<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:id="@+id/promoted_text_banner_layout_one" android:layout_width="match_parent" android:layout_height="wrap_content">
    <RelativeLayout android:id="@+id/content_layout" android:paddingTop="8dp" android:layout_width="match_parent" android:layout_height="wrap_content" android:paddingStart="16dp" android:paddingEnd="16dp">
        <include android:id="@+id/contextual_menu_anchor" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="4dp" android:layout_alignParentTop="true" android:layout_marginStart="8dp" android:layout_alignParentEnd="true" layout="@layout/contextual_menu_anchor"/>
        <com.google.android.libraries.youtube.common.ui.YouTubeTextView android:textSize="@dimen/small_font_size" android:textColor="?attr/adText2" android:ellipsize="end" android:id="@+id/subtitle" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="1dp" android:maxLines="2" android:layout_below="@+id/title_layout" android:layout_toStartOf="@+id/contextual_menu_anchor" android:layout_alignParentStart="true" app:robotoFont="0"/>
        <include android:id="@+id/ad_attribution" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="4dp" android:layout_marginBottom="15dp" android:layout_below="@+id/subtitle" android:layout_marginEnd="6dp" android:layout_alignParentStart="true" layout="@layout/standalone_ad_badge_small"/>
        <com.google.android.libraries.youtube.common.ui.YouTubeTextView android:textSize="@dimen/small_font_size" android:textColor="?attr/adText1" android:ellipsize="end" android:id="@+id/byline" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="1dp" android:maxLines="1" android:layout_below="@+id/subtitle" android:layout_toEndOf="@+id/ad_attribution" app:robotoFont="0"/>
        <FrameLayout android:id="@+id/title_layout" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_alignParentTop="true" android:layout_toStartOf="@+id/contextual_menu_anchor" android:layout_alignParentStart="true">
            <LinearLayout android:orientation="horizontal" android:layout_width="wrap_content" android:layout_height="wrap_content">
                <com.google.android.libraries.youtube.common.ui.YouTubeTextView android:textSize="@dimen/medium_font_size" android:textColor="?attr/adBlue" android:ellipsize="end" android:id="@+id/title" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="1dp" android:maxLines="1" android:lines="1" android:layout_weight="1" android:layout_marginEnd="6dp" app:robotoFont="0"/>
                <com.google.android.apps.youtube.app.common.widget.TintableImageView android:theme="@style/Widget.YouTube.Control.Light" android:id="@+id/title_icon" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="15dp" android:layout_marginTop="5dp" android:layout_weight="0" android:contentDescription="@null"/>
            </LinearLayout>
        </FrameLayout>
    </RelativeLayout>
    <View android:id="@+id/click_overlay" android:layout_width="match_parent" android:layout_height="match_parent"/>
</FrameLayout>
