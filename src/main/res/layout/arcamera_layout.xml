<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:orientation="vertical" android:id="@+id/camera_container" android:layout_width="match_parent" android:layout_height="match_parent">
    <TextureView android:id="@+id/camera_texture" android:layout_width="wrap_content" android:layout_height="wrap_content"/>
    <WebView android:id="@+id/web_ui_view" android:layout_width="match_parent" android:layout_height="match_parent"/>
    <LinearLayout android:orientation="vertical" android:layout_width="wrap_content" android:layout_height="wrap_content">
        <TextView android:id="@+id/title" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="Ar Camera Layout"/>
        <Button android:id="@+id/camera_button" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="camera button"/>
    </LinearLayout>
</FrameLayout>
